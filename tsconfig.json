{
	"include":
	[
		"*.tsx"
	],
	"compilerOptions":
	{
		"noEmit": false,
		"outDir": "release/dev/",
		"pretty": true,
"jsx": "react",

		"strict": true,
		"target": "ES6",
		"lib":  [ "ES6" ],

		"allowJs": false,
		"checkJs": false,

		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true
	}
}
